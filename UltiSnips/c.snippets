###########################################################################
#                            TextMate Snippets                            #
###########################################################################

priority -49

snippet for "for loop (for)"
for (${2:i} = 0; $2 < ${1:count}; $2${3:++})
{
	${VISUAL}${0}
}
endsnippet

snippet #i
#include <${1:stdio}.h>
endsnippet

snippet #i"
#include "${1:}"
endsnippet

# vim:ft=snippets:
